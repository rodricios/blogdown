//blogdown's schema


{
    "type":"schema",
    "version": 0.0,
    "classes": ["posts", "post", "nav"],
    "class_schemas": {
        "posts": ("post_id", "post_title",
                  "description", "pubdate",
                  "tags", "author"),
        "post": ("id", "title", "content", "date", "tags", "author"),
        "header": ("title", "tagline"),
        "nav": ("menu")
    }
}
